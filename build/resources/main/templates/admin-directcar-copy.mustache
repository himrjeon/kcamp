{{>layout/headeradmin}}

                <div class="col-sm-8 col-sm-offset-1">
                    <div class="post">
                        <div class="post-header font-alt">
                            <h1 class="post-title">SHOW ROOM 차량 등록</h1>
                            <div class="post-meta">By&nbsp;<a href="#">관리자</a>
                            </div>
                        </div>
                        <div class="post-entry">
                            <form id="insertForm" action="/showroom/upload" method="post" enctype="multipart/form-data">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        상품제목<input class="form-control" type="text" id="title" name="title" placeholder="제목" value="{{directcar.title}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        판매여부
                                        <select id="carsalecheck" name="carsalecheck" class="form-control">
                                            <option value="계약및선적대기">계약및선적대기</option>
                                            <option value="이동중">이동중</option>
                                            <option value="입항 및 통관중">입항 및 통관중</option>
                                            <option value="인증진행중">인증진행중</option>
                                            <option value="상품화완료">상품화완료</option>
                                            <option value="판매중">판매중</option>
                                            <option value="판매완료">판매완료</option>
                                        </select>
                                    </div>
                                </div>
                                <input type="hidden" name="id" id="id" value="{{directcar.id}}">
                                <input type="hidden" name="filePath" id="filePath" value="{{directcar.filePath}}">
                                <input type="hidden" name="filePath2" id="filePath2" value="{{directcar.filePath2}}">
                                <input type="hidden" name="filePath3" id="filePath3" value="{{directcar.filePath3}}">
                                <input type="hidden" name="filePath4" id="filePath4" value="{{directcar.filePath4}}">
                                <input type="hidden" name="filePath5" id="filePath5" value="{{directcar.filePath5}}">
                                <input type="hidden" name="filePath6" id="filePath6" value="{{directcar.filePath6}}">
                                <input type="hidden" name="filePath7" id="filePath7" value="{{directcar.filePath7}}">
                                <input type="hidden" name="filePath8" id="filePath8" value="{{directcar.filePath8}}">
                                <input type="hidden" name="filePath9" id="filePath9" value="{{directcar.filePath9}}">
                                <input type="hidden" name="filePath10" id="filePath10" value="{{directcar.filePath10}}">
                                <input type="hidden" name="filePath11" id="filePath11" value="{{directcar.filePath11}}">
                                <input type="hidden" name="filePath12" id="filePath12" value="{{directcar.filePath12}}">
                                <input type="hidden" name="filePath13" id="filePath13" value="{{directcar.filePath13}}">
                                <input type="hidden" name="filePath14" id="filePath14" value="{{directcar.filePath14}}">
                                <input type="hidden" name="filePath15" id="filePath15" value="{{directcar.filePath15}}">
                                <input type="hidden" name="filePath16" id="filePath16" value="{{directcar.filePath16}}">
                                <input type="hidden" name="filePath17" id="filePath17" value="{{directcar.filePath17}}">
                                <input type="hidden" name="filePath18" id="filePath18" value="{{directcar.filePath18}}">
                                <input type="hidden" name="filePath19" id="filePath19" value="{{directcar.filePath19}}">
                                <input type="hidden" name="filePath20" id="filePath20" value="{{directcar.filePath20}}">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        간단소개내용(3줄이하)
                                        <textarea class="form-control" id="summernote1"
                                                  name="subcontent" placeholder="content" rows="3">{{directcar.subcontent}}</textarea>
                                    </div>
                                </div>
                                <br><br>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        상세내용
                                        <textarea class="form-control" id="summernote"
                                                  name="content" placeholder="content" rows="7">{{{directcar.content}}}</textarea>
                                    </div>
                                </div>
                            </form>

                            <div class="col-sm-12">
                                <div class="form-group">
                                    <a href="/admin/directcar" role="button" class="btn btn-g btn-round">취소</a>
                                    <button type="button" class="btn btn-primary" id="showroom-btn-copy">차량복사</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>



{{>layout/footeradmin}}
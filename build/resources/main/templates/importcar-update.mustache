                                    </div>
                                </div>
                                <br><br>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        상세내용
                                        <textarea class="form-control" id="summernote"
                                                  name="content" placeholder="content" rows="7">{{{importcar.content}}}</textarea>
                                    </div>
                                </div>

                            </form>
                            <div class="col-sm-12">
                                <div class="form-group">
                                    <a href="/admin/importcar" role="button" class="btn btn-g btn-round">취소</a>
                                    <button type="button" class="btn btn-primary" id="car-btn-update">수정하기</button>
                                </div>
                            </div>

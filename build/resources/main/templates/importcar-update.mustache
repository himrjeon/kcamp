                        <ul class="icon-list">
                            <li><a href="/admin/popup">팝업 관리</a></li>
                        </ul>
                    </div>
                </div>
                <div class="col-sm-8 col-sm-offset-1">
                    <div class="post">
                        <div class="post-header font-alt">
                            <h1 class="post-title">차량 수정</h1>
                            <div class="post-meta">By&nbsp;<a href="#">관리자</a>
                            </div>
                        </div>
                        <div class="post-entry">
                            <form id="insertForm" action="/importcar/upload" method="post" enctype="multipart/form-data">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        상품제목<input class="form-control" type="text" id="title" name="title" placeholder="제목" value="{{importcar.title}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        판매여부
                                        <select id="carsalecheck" name="carsalecheck" class="form-control">
                                            {{#importcar.carsalecheck}}
                                                <option value="{{importcar.carsalecheck}}" selected="selected">{{importcar.carsalecheck}}</option>
                                            {{/importcar.carsalecheck}}
                                            <option value="계약완료">계약완료</option>
                                            <option value="입항예정">입항예정</option>
                                            <option value="통관진행중">톻관진행중</option>
                                            <option value="인증중">인증중</option>
                                            <option value="판매중">판매중</option>
                                            <option value="판매완료">판매완료</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        가격
                                        <input class="form-control" type="text" id="carprice" name="carprice" onKeyup="inputPriceNumberAutoComma(this);" placeholder="가격(원)" value="{{importcar.carprice}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        제조사
                                        <input class="form-control" type="text" id="carmanufacturer" name="carmanufacturer" placeholder="제조사" value="{{importcar.carmanufacturer}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        모델명
                                        <input class="form-control" type="text" id="carmodel" name="carmodel" placeholder="모델명" value="{{importcar.carmodel}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        세부모델명
                                        <input class="form-control" type="text" id="carmodeldetail" name="carmodeldetail" placeholder="세부모델명" value="{{importcar.carmodeldetail}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        등급명
                                        <input class="form-control" type="text" id="cartrim" name="cartrim" placeholder="등급명" value="{{importcar.cartrim}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        세부등급명
                                        <input class="form-control" type="text" id="cartrimdetail" name="cartrimdetail" placeholder="세부등급명" value="{{importcar.cartrimdetail}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        연식
                                        <input class="form-control" type="text" id="caryear" name="caryear" placeholder="연식" value="{{importcar.caryear}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        주행거리
                                        <input class="form-control" type="text" id="cardistance" name="cardistance" onKeyup="inputNumberAutoComma(this);" placeholder="주행거리(km)" value="{{importcar.cardistance}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        배기량
                                        <input class="form-control" type="text" id="carpower" name="carpower" onKeyup="inputNumberAutoComma(this);" placeholder="배기량(cc)" value="{{importcar.carpower}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        변속기
                                        <input class="form-control" type="text" id="cartransmission" name="cartransmission" placeholder="변속기" value="{{importcar.cartransmission}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        색상
                                        <input class="form-control" type="text" id="carcolor" name="carcolor" placeholder="색상" value="{{importcar.carcolor}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        연료
                                        <input class="form-control" type="text" id="carfuel" name="carfuel" placeholder="연료"  value="{{importcar.carfuel}}">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        구동방식
                                        <input class="form-control" type="text" id="cartractor" name="cartractor" placeholder="구동방식" value="{{importcar.cartractor}}">
                                    </div>
                                </div>
                                <input type="hidden" name="id" id="id" value="{{importcar.id}}">
                                <input type="hidden" name="filePath" id="filePath" value="{{importcar.filePath}}">
                                <input type="hidden" name="filePath2" id="filePath2" value="{{importcar.filePath2}}">
                                <input type="hidden" name="filePath3" id="filePath3" value="{{importcar.filePath3}}">
                                <input type="hidden" name="filePath4" id="filePath4" value="{{importcar.filePath4}}">
                                <input type="hidden" name="filePath5" id="filePath5" value="{{importcar.filePath5}}">
                                <input type="hidden" name="filePath6" id="filePath6" value="{{importcar.filePath6}}">

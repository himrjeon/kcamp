                                <input type="hidden" name="filePath20" id="filePath20" value="{{importcar.filePath20}}">
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        차량대표사진
                                        <input class="form-control" type="file" id="file" name="file">
                                    </div>
                                </div>
                                <div class="col-sm-12">
                                    <div class="form-group">
                                        차량이미지(1)
                                        <input class="form-control" type="file" id="file2" name="file2">

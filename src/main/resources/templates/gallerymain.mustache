{{>layout/headermain}}
<div class="main">
    <section class="module bg-dark-60 about-page-header" data-background="/image/inst_pn_test3.jpg">
        <div class="container">
            <div class="row">
                <div class="col-sm-6 col-sm-offset-3">
                    <h2 class="module-title font-alt">K-CAMPING CAR</h2>
                </div>
            </div>
        </div>
    </section>
    <section class="module-medium">
        <div class="container">
            <ul class="works-grid works-grid-gut works-hover-d" id="works-grid">
                {{#galleryList}}
